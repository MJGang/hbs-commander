    "magic-string": "^0.30.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "prettier": "^3.4.1",
    "rollup": "^4.31.0",
    "vitest": "^3.0.2"
  }
}