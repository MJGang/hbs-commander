{
  "name": "hbs-commander",
  "version": "2.0.3",
  "description": "A library for intelligently inserting and replacing Handlebars templates with context-aware placement",
  "main": "dist/hbs-commander.cjs.js",
  "module": "dist/hbs-commander.esm.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "format": "prettier --write .",
    "build": "rollup -c",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "handlebars",
    "template",
    "insertion",
    "replacement",
    "placement"
  ],
  "author": "MJGang",
  "license": "MIT",
  "dependencies": {
